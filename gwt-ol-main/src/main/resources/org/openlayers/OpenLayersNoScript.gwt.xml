<module>
    <inherits name="com.google.gwt.user.User" />
    <inherits name="com.google.gwt.json.JSON" />
    <inherits name="com.google.gwt.xml.XML" />

    <entry-point class="org.openlayers.ol.OpenLayersEntryPoint" />

    <define-linker name="openLayersScriptInjector" class="org.openlayers.linker.OpenLayersScriptInjector" />
